# Server Configuration
PORT=3000
NODE_ENV=development

# Confluence Configuration
CONFLUENCE_DOMAIN=your-domain.atlassian.net
CONFLUENCE_CLIENT_ID=your-client-id
CONFLUENCE_CLIENT_SECRET=your-client-secret

# OAuth2 Configuration
AUTH_TOKEN_URL=https://auth.atlassian.com/oauth/token
AUTH_RESOURCES_URL=https://api.atlassian.com/oauth/token/accessible-resources
REDIRECT_URI=http://localhost:3000/oauth/callback
# Logging
LOG_LEVEL=info 